<view class="container">
        <image style="width: 60px; height: 60px;margin-top: 30px;" src="../../images/success.png" />
        <view class="success-title">预订成功</view>
        <view class="line"></view>
        <view class='title'>{{camperCarDetail.CamperTypeName}}</view>
        <view class='subtitle'>{{camperCarDetail.CampShortName}}</view>

        <view class="line"></view>


        <view class="order-info">
                <view class='info-title'>预订人</view>
                <view class='info-content'>{{orderInfo.nickName}}</view>
        </view>
        <view class="order-info">
                <view class='info-title'>入住日期</view>
                <view class='info-content'>{{camperCarDetail.BTimeDate}} ({{camperCarDetail.CheckInTime}}后)</view>
        </view>
        <view class="order-info">
                <view class='info-title'>退房日期</view>
                <view class='info-content'>{{camperCarDetail.ETimeDate}} ({{camperCarDetail.CheckOutTime}}前)</view>
        </view>
        <view class="line"></view>
        <view bindtap="finish">
                <button class="login-btn" formType="submit">完成</button>
        </view>
</view>